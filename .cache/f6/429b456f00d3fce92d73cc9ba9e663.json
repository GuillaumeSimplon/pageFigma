   background-color: white;\n    padding: 13px 100px;\n    margin: 15px;\n    text-align: center;\n    text-decoration: none;\n    color: black;\n    font-weight: 500;\n}\n\n.listePost>a:hover {\n    color: #f7c552;\n}\n\n\n\n\n\n/* DEUXIEME BLOC du BLOG */\n.post {\n    display: flex;\n    flex-direction: column;\n    justify-content: start;\n    align-items: center;\n    padding: 10% 0;\n}\n\n/* CONTAINER */\n.postImg {\n    /* max-width: 770px; */\n    max-width: 1000px;\n    margin: 0 20px;\n    display: flex;\n    flex-direction: column;\n    align-items: start;\n    justify-content: start;\n    background-color: white;\n}\n\n/* IMAGE */\n.postImg>img {\n    width: 100%;\n    height: auto;\n}\n\n/* TITRE H3 */\n.postImg>h3 {\n    text-align: left;\n    margin: 20px;\n    font-size: 22px;\n    font-weight: 600;\n}\n\n/* WIDGET */\n#titreWidget {\n    font-size: 14px;\n    color: #3dc1cd;\n    margin: 0 20px;\n    font-weight: 500;\n}\n\n/* PARAGRAPHE */\n.postImg>p {\n    color: grey;\n    font-size: 16px;\n    margin: 20px 20px 50px 20px;\n    line-height: 150%;\n}\n\n\n\n\n\n\n/* VERSION ORDI */\n\n@media screen and (min-width: 780px) {\n\n    .blog {\n        flex-direction: row;\n    }\n\n    /* PREMIER BLOC du BLOG */\n    .listePost {\n        align-items: start;\n        padding: 0 5%;\n    }\n\n    /* TITRE H2 */\n    .listePost>h2 {\n        margin-top: 20%;\n        text-align: left;\n        font-size: 22px;\n    }\n\n    /* LIGNE */\n    .lignePost {\n        width: 90%;\n    }\n\n    /* PARAGRAPHE du PREMIER BLOC du BLOG */\n    .listePost>p {\n        text-align: left;\n        font-size: 16px;\n    }\n\n    /* LIENS */\n    .listePost>a {\n        min-width: 90%;\n        background-color: white;\n        margin: 0;\n        padding: 20px 0;\n        text-align: center;\n        font-weight: 600;\n    }\n\n\n}"},"lineCount":null}},"error":null,"hash":"4de65faa8b29163392bd32e96659e613","cacheData":{"env":{}}}